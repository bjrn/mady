{
  "name": "mady",
  "version": "2.2.0",
  "description": "Easy-to-use tool to manage and translate ICU MessageFormat messages",
  "bin": {
    "mady": "lib/mady.js"
  },
  "main": "lib/translate.js",
  "engines": {
    "node": ">=4"
  },
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "i18n",
    "MessageFormat",
    "translation",
    "locales",
    "translator"
  ],
  "homepage": "https://github.com/guigrpa/mady#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/mady/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/mady.git"
  },
  "scripts": {
    "start": "babel-node src/server/startup --dir src/locales",
    "startAlt": "babel-node src/server/startup --dir tools/locales",
    "startProd": "node lib/mady --dir src/locales",
    "compile": "rm -rf ./lib && mkdir lib && babel --out-dir lib --ignore \"**/__mocks__/**\",\"**/__tests__/**\" src && cp src/api.js.flow lib/translate.js.flow",
    "updateSchemaJson": "babel-node src/server/gqlUpdateSchema",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "buildSsrWatch": "rm -rf ./public/ssr && rm -rf ./lib/server/ssr && cross-env SERVER_SIDE_RENDERING=true webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks --watch",
    "buildSsr": "rm -rf ./public/ssr && rm -rf ./lib/server/ssr && cross-env SERVER_SIDE_RENDERING=true NODE_ENV=production webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks",
    "buildClient": "rm -rf ./public/assets && cross-env NODE_ENV=production webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks",
    "buildClientWatch": "rm -rf ./public/assets && webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks --watch",
    "buildAnalysis": "rm -rf ./public/assets && cross-env ANALYZE_BUNDLE=true webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks",
    "buildAnalysisProd": "rm -rf ./public/assets && cross-env NODE_ENV=production ANALYZE_BUNDLE=true webpack --config ./src/server/webpackConfig.cjs --progress --display-chunks",
    "build": "node package && npm run lint && npm run flow && npm run compile && npm run buildClient && npm run buildSsr && npm run test && npm run docs && npm run xxl",
    "travis": "npm run compile && npm run testCovFullExceptMin",
    "lint": "eslint src",
    "flow": "flow check || exit 0",
    "xxl": "xxl",
    "test": "npm run testCovFull",
    "testCovFull": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFullExceptMin": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFast": "npm run testCovPrepare && npm run testDev && npm run testCovReport",
    "jest": "jest --watch --coverage",
    "jest-html": "jest-html --snapshot-patterns \"src/**/*.snap\"",
    "jestDebug": "node --debug-brk --inspect node_modules/.bin/jest -i",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "cross-env NODE_ENV=development jest --coverage && mv .nyc_output/coverage-final.json .nyc_tmp/coverage-dev.json",
    "testProd": "cross-env NODE_ENV=production jest --coverage && mv .nyc_output/coverage-final.json .nyc_tmp/coverage-prod.json",
    "testMin": "cross-env TEST_MINIFIED_LIB=1 jest --coverage && mv .nyc_output/coverage-final.json .nyc_tmp/coverage-min.json",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "babel-relay-plugin": "0.9.3",
    "bluebird": "3.4.6",
    "commander": "2.9.0",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "diveSync": "0.3.0",
    "ejs": "2.5.2",
    "express": "4.14.0",
    "express-graphql": "0.5.4",
    "fs-extra": "0.30.0",
    "graphql": "0.7.0",
    "graphql-relay": "0.4.3",
    "lodash": "4.16.0",
    "messageformat": "1.0.2",
    "opn": "4.0.2",
    "slash": "1.0.0",
    "storyboard": "^3.0.0",
    "storyboard-listener-console": "^3.0.0",
    "storyboard-listener-ws-server": "^3.0.0",
    "timm": "1.2.1",
    "uglify-js": "2.7.3",
    "uuid": "3.0.1"
  },
  "peerDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-plugin-react-intl": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "bundle-loader": "0.5.4",
    "coveralls": "2.11.14",
    "cross-env": "2.0.1",
    "css-loader": "0.28.0",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-flowtype": "2.20.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-docs": "1.4.0",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "file-loader": "0.11.1",
    "flow-bin": "0.36.0",
    "font-awesome": "4.6.3",
    "giu": "0.9.2",
    "isomorphic-relay": "0.7.3",
    "jest": "19.0.2",
    "jest-html": "^1.3.2",
    "moment": "2.14.0",
    "node-sass": "4.5.0",
    "nyc": "8.3.0",
    "react": "15.4.1",
    "react-addons-perf": "15.4.1",
    "react-dom": "15.4.1",
    "react-intl": "2.1.5",
    "react-relay": "0.9.3",
    "react-test-renderer": "15.4.1",
    "sass-loader": "4.1.1",
    "storyboard-listener-browser-extension": "^3.0.0",
    "storyboard-listener-ws-client": "^3.0.0",
    "style-loader": "0.16.1",
    "tiny-cookie": "0.5.5",
    "tinycolor2": "1.4.1",
    "webpack": "2.4.1",
    "webpack-bundle-analyzer": "2.4.0",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "xxl": "^1.2.0"
  },
  "jest": {
    "testRegex": "src/.*__tests__/.*\\.(test|spec)\\.(js|jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|less|sass)$": "<rootDir>/test/emptyObject.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/emptyString.js"
    },
    "coverageDirectory": ".nyc_output",
    "coverageReporters": [
      "json",
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-html"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/__mocks__/**",
      "!src/locales/**",
      "!src/localesBackup/**",
      "!src/exampleLocales.js",
      "!src/mady.js",
      "!src/server/importData.js",
      "!src/client/components/999-reactIntlExample.js",
      "!src/client/components/fetchLangBundle.js"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js"
  }
}
